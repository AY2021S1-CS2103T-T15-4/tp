<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.image.ImageView?>
<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" prefHeight="100.0" prefWidth="95.0">
    <HBox styleClass="tab_parameter_style">
        <Button onAction="#handleClick" fx:id="internshipButton" mnemonicParsing="false" styleClass="tab_button_style">
            <graphic>
                <VBox styleClass="tab_container_style">
                    <ImageView fitHeight="27.0" fitWidth="27.0" fx:id="internshipIcon" translateY="10" translateX="2"/>
                    <Label text="Internship" styleClass="tab_label_style"/>
                    <HBox styleClass="tab_line_style"/>
                </VBox>
            </graphic>
        </Button>
        <VBox fx:id="internship" styleClass="tab_bar_style"/>
    </HBox>
    <HBox styleClass="tab_parameter_style">
        <Button translateY = "-13.5" onAction="#handleClick" fx:id="companyButton" mnemonicParsing="false" styleClass="tab_button_style">
            <graphic>
                <VBox styleClass="tab_container_style">
                    <ImageView fitHeight="26.0" fitWidth="26.0" fx:id="companyIcon" translateY="5"/>
                    <Label text="Company" styleClass="tab_label_style"/>
                    <HBox styleClass="tab_line_style"/>
                </VBox>
            </graphic>
        </Button>
        <VBox fx:id="company" styleClass="tab_bar_style"/>
    </HBox>
    <HBox styleClass="tab_parameter_style">
        <Button translateY = "-27" onAction="#handleClick" fx:id="userButton" mnemonicParsing="false" styleClass="tab_button_style">
            <graphic>
                <VBox  styleClass="tab_container_style">
                    <ImageView fitHeight="23.0" fitWidth="23.0" fx:id="userIcon" translateY="5"/>
                    <Label text="Profile" styleClass="tab_label_style"/>
                    <HBox styleClass="tab_line_style"/>
                </VBox>
            </graphic>
        </Button>
        <VBox fx:id="user" styleClass="tab_bar_style"/>
    </HBox>
</fx:root>
