<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.TextFlow?>
<?import javafx.geometry.Insets?>
<fx:root type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" styleClass="internship_information_display_container">
    <VBox styleClass="internship_information_title">
        <padding>
            <Insets top="10" right="10" bottom="10" left="10" />
        </padding>
        <TextFlow fx:id="title"/>
    </VBox>
    <VBox styleClass="internship_information_description">
        <padding>
            <Insets top="10" right="10" bottom="10" left="10" />
        </padding>
        <TextFlow fx:id="description"/>
    </VBox>
</fx:root>

